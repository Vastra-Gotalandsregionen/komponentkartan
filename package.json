    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-spec-reporter": "~0.0.32",
    "karma-typescript": "^3.0.13",
    "ng-packagr": "^3.0.0-rc.2",
    "protractor": "^5.4.0",
    "smoothscroll-polyfill": "^0.4.3",
    "ts-node": "~3.2.0",
