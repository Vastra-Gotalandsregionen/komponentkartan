{
  "name": "vgr-komponentkartan",
  "version": "0.0.67",
  "author": "Västra Götalandsregionen",
  "scripts": {
    "[start-app]": "concurrent \"npm run _watch-ts\" \"npm run _local-webserver\" \"npm run _watch-css\" \"npm run _watch-demo-app-css\"",
    "_compile-ts": "tsc -p tsconfig.demo-app.json",
    "_watch-ts": "tsc -p tsconfig.demo-app.json -w",
    "_local-webserver": "lite-server",
    "_compile-css": "node-sass --include-path content content/komponentkartan.scss content/komponentkartan.css",
    "_compile-demo-app-css": "node-sass --include-path content demo-app/content/demo-app.scss demo-app/content/demo-app.css",
    "_watch-css": "nodemon -e scss --watch content/  --exec \"npm run _compile-css\"",
    "_watch-demo-app-css": "nodemon -e scss --watch demo-app/content/ --watch content/  --exec \"npm run _compile-demo-app-css\"",
    "_clean_build_output": "rimraf ./BuildOutput"
  },
  "dependencies": {
    "@angular/common": "^4.3.1",
    "@angular/compiler": "^4.3.1",
    "@angular/core": "^4.3.1",
    "@angular/forms": "^4.3.1",
    "@angular/http": "^4.3.1",
    "@angular/platform-browser": "^4.3.1",
    "@angular/platform-browser-dynamic": "^4.3.1",
    "@angular/router": "^4.3.1",
    "@angular/upgrade": "^4.3.1",
    "@types/core-js": "0.9.36",
    "@types/jquery": "^2.0.48",
    "@types/node": "7.0.7",
    "core-js": "~2.4.1",
    "jquery": "^3.1.1",
    "ngx-perfect-scrollbar": "~4.4.0",
    "reflect-metadata": "~0.1.10",
    "rxjs": "^5.4.1",
    "systemjs": "0.19.47",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "^1.3.2",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/language-service": "^4.0.0",
    "@types/jasmine": "2.5.45",
    "body-parser": "^1.16.1",
    "codelyzer": "~3.0.1",
    "concurrently": "~3.3.0",
    "intl": "^1.2.5",
    "jasmine": "2.5.3",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.2.2",
    "node-sass": "~4.5.0",
    "nodemon": "^1.11.0",
    "protractor": "~5.1.2",
    "rimraf": "2.6.1",
    "task-runner-js": "^2.1.4",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3"
  },
  "-vs-binding": {
    "ProjectOpened": [
      "_watch-typescript",
      "_watch-css"
    ]
  }
}
